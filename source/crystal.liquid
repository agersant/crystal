return {
	type = "spritesheet",
	content = {
		texture = "{{ sheet_image | replace: "\", "/" }}",
		frames = {
			{%- for frame in frames %}
			frame_{{ frame.index }} = { x = {{ frame.x }}, y = {{ frame.y }}, w = {{ frame.width }}, h = {{ frame.height }}, },
			{%- endfor %}
		},
		animations = {
			{%- for animation in animations %}
			["{{ animation.name }}"] = {
				loop = {{ animation.is_looping }},
				sequences = {
					{%- for sequence in animation.sequences %}
					{
						direction = "{{ sequence.direction }}",
						frames = {
							{%- for keyframe in sequence.keyframes %}
							{ id = "frame_{{ keyframe.frame.index }}", duration = {{ keyframe.duration | divided_by: 1000.0 }}, ox = {{ keyframe.top_left_offset_x | times: -1 }}, oy = {{ keyframe.top_left_offset_y | times: -1 }},
							{%- assign num_hitboxes = keyframe.hitboxes | size -%}
							{%- if num_hitboxes > 0 -%}
							tags = {
								{%- for hitbox in keyframe.hitboxes -%}
								["{{ hitbox.name }}"] = { rect = { x = {{ hitbox.left }}, y = {{ hitbox.top }}, w = {{ hitbox.width }}, h = {{ hitbox.height }} } },
								{%- endfor -%}
							},
							{%- endif -%}
							},
							{%- endfor %}
						},
					},
					{%- endfor %}
				},
			},
			{%- endfor %}
		},
	},
};
